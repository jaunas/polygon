${CND_DISTDIR}/${CND_CONF}/${CND_PLATFORM}/polygon: ${OBJECTFILES}
	${MKDIR} -p ${CND_DISTDIR}/${CND_CONF}/${CND_PLATFORM}
	${LINK.cc} -o ${CND_DISTDIR}/${CND_CONF}/${CND_PLATFORM}/polygon ${OBJECTFILES} ${LDLIBSOPTIONS} -lglfw -lGL -lX11 -lpthread -lXrandr -lXi -ldl

